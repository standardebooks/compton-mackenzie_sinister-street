		<dc:title id="title">Sinister Street</dc:title>
		<meta property="file-as" refines="#title">Sinister Street</meta>
		<dc:subject id="subject-1">Bildungsromans</dc:subject>
		<dc:subject id="subject-2">Illegitimate children -- Fiction</dc:subject>
		<dc:subject id="subject-3">England -- Social life and customs -- Fiction</dc:subject>
